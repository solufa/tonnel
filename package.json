{
  "private": true,
  "scripts": {
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:client": "npm run dev --prefix packages/client",
    "dev:server": "npm run dev --prefix packages/server",
    "generate": "run-p generate:*",
    "generate:client": "npm run generate --prefix packages/client",
    "generate:server": "npm run generate --prefix packages/server",
    "lint": "run-p lint:*",
    "lint:client": "npm run lint:fix --prefix packages/client",
    "lint:server": "npm run lint:fix --prefix packages/server",
    "test": "run-p test:*",
    "test:client": "npm run test --prefix packages/client",
    "test:server": "npm run test --prefix packages/server",
    "typecheck": "run-p typecheck:*",
    "typecheck:client": "npm run typecheck --prefix packages/client",
    "typecheck:server": "npm run typecheck --prefix packages/server"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "notios": "^0.4.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
